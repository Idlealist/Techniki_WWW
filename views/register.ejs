<h1>Register</h1>
<% if (message) {%>
    <%= message%>
<%}%>
<form action="/register" method="POST">
    <input type="text" name="username" placeholder="Username" value="<%= username ? username : ''%>"  required><br>
    <input type="password" name="password" placeholder="Password" value="<%= password ? password : ''%>" required><br>
    <button type="submit">Register</button>
</form>
